	CaptureScreen(name:screenshotName)
end hook_before_feature

to handle hook_after_scenario
	Log "****************** After Scenario Starts Here: Closing Power Point******************"
	put "After Scenario: Close PPT and Generate Report" into Description
	put "Power point should get closed" into Expected
	Set the searchRectangle to ()
	Repeat while ImageFound("close_Icon")
		If ImageFound(4, "close_Icon")
			click "close_Icon"
			If ImageFound(4, "Microsoft_Close_Dialogue")
				
				if ImageFound(4, "dont_save_button")
					click "dont_save_button"	
					put "power point closed" into Actual
					LogSuccess Actual
					put Reporter("Steps",scenarioName,Description, Expected, Actual, "Pass","Yes") into flag
				else
					put "dont_save_button image not found" into Actual
					Log Actual
					put Reporter("Steps",scenarioName,Description, Expected, Actual, "Fail","Yes") into flag
				End If
				if ImageFound(1, "recovery_dialog")
					click "recovery_dialog"	
					put "power point closed" into Actual
					LogSuccess Actual
					put Reporter("Steps",scenarioName,Description, Expected, Actual, "Pass","Yes") into flag
					
				End If
			else
				put "Microsoft close ppt dialog image not found" into Actual
				Log Actual
				put Reporter("Steps",scenarioName, Description, Expected, Actual, "Fail","Yes") into flag
			End If
			
		else
			put "Close ppt image not found" into Actual
			put Reporter("Steps",scenarioName, Description, Expected, Actual, "Fail","Yes") into flag
			Log "Close ppt image not found"
		End If	
	End repeat
	Log "****************** After Scenario Completed: Power point closed******************"
end hook_after_scenario
to handle Power_point_is_open
	Log "****************** Verification if power point is open******************"
	put "Verify if power point is open" into Description
	put "Power point should be open on screen" into Expected
	Try
		If ImageFound(10, "powerpointIcon")
			if ImageFound(20,"PowerPointHeader")
				put "Power point is open on screen" into Actual
				Log "Power point is open on screen"
				put Reporter("Steps",scenarioName, Description, Expected, Actual,"Pass","No") into flag
				
			else 
				
				Click "powerpointIcon"
				WaitFor 10, "PowerPointHeader"
				put Reporter("Steps",scenarioName, Description, Expected, Actual, "Fail","No") into flag
			end If
		Else
			Log "powerpoint not open"
			TypeText WindowsKey , "r" // to open run command 
			TypeText DeleteKey // delete the previous data 
			TypeText "powerpnt.exe",Return
			wait 10
		End If	
	Catch exception
		LogError "Something went wrong "& exception
	End try
	Log "****************** Verificationpower point open completed*****************"
end Power_point_is_open

