to handle User_Inserts_STR_from_fixed_elements_in_Basic_Slide_STR
	params elementName, value
	put "insert element :"&elementName&" into agenda slide and verify inserted correctly with value"&value into Description
	put "element :"&elementName&" ishould be inserted in to tile slide" into Expected
	set the SearchRectangle to ()
	Try
		if elementName="Footnote"
			if ImageFound(text:"Basic", waitFor:1)
				click text:"Basic"
			else
				click text: "Click"
			end if
		end if
		if ImageFound(imageName:"fixed_elements_options", waitFor:3)
			Log "fixed elements dropdown open for presentation"
		else
			click "fixed_elements_ribbon"
		end if
		put Library("addFixedElement",elementName) into flag
		if flag
			if elementName="Sticker"
				set the SearchRectangle to ()
				put Library("addSticker",value) into flag
				if flag
					Log "Sticker inserted on slide, time to verify its value"
					if ImageFound(imageName:"cross_icons", waitFor:5)
						click "cross_icons"
						set the SearchRectangle to ()
						set the SearchRectangle to (300,300,1600,400)
						if ImageFound(text:value, contrast:On, waitFor:10)
							click text:value
							put "Sticker: "&value&" added to slide" into Actual
							LogSuccess Actual
							put reporter(Steps,scenarioName, Description, Expected,Actual, "Pass","No") into flag
						else
							put "Sticker: "&value&" not found in slide" into Actual
							LogError Actual
							put reporter(Steps,scenarioName, Description, Expected,Actual, "Fail","No") into flag
							
						end if
					else
						put "Unable to cross icon from popup" into Actual
						LogError Actual
						put reporter(Steps,scenarioName, Description, Expected,Actual, "Fail","No") into flag
					end if
					
				else
